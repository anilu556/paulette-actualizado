      "flex-layout.row#imaginfoizq-b-tab",
      "flex-layout.row#infcardizq-b-tab"
    ],
    "props":{
      "blockClass": "inf-card-cont-tab"
    }
  },"flex-layout.row#imaginfoizq-b-tab":{
    "children":[
      "image#imageinfocardizq-b-tab"
    ],"props":{
      "horizontalAlign":"left",
      "blockClass": "img-info-cont-tab"
    }
  },"image#imageinfocardizq-b-tab":{
    "props":{
      "src":"arquivos/infocard1nosotros.png"
      // "blockClass": "img-info"
    }
  },

  "flex-layout.row#infcardizq-b-tab":{
    "children":[
      "info-card#pruebaizq-b-tab"
    ],"props":{
      // "width": "45%"
    }
  },"info-card#pruebaizq-b-tab":{
    "props":{
      "id": "info-card-example",
        "isFullModeStyle": false,
        "textPosition": "left",
        "textMode":"rich-text",
        "headline": "# BOUTIQUES CAFÉ",
        "subhead": "Visítanos y selecciona tus postres favoritos para llevar a casa o disfrutarlos ahí mismo.",
        "callToActionText": "VER MÁS",
        "callToActionUrl": "/storelocator",
        "textAlignment": "center",
        "blockClass":"inforpuebaizq"
    }
  },

  /* Info card 2 tablet*/

  "flex-layout.row#infotxtder-boutiq-tablet":{
    "children":[
      "flex-layout.row#infcardder-b-tab",
      "flex-layout.row#imginfoder-b-tab"
    ],
    "props":{
      "blockClass": "inf-card-cont-tab"
    }
  },"flex-layout.row#imginfoder-b-tab":{
    "children":[
      "image#mageinfocardder-b-tab"
    ],"props":{
      "horizontalAlign":"left",
      "blockClass": "img-info-cont-tad-der"
    }
  },"image#mageinfocardder-b-tab":{
    "props":{
      "src":"arquivos/infocard2nosotros.png"
      // "blockClass": "img-info"
    }
  },

  "flex-layout.row#infcardder-b-tab":{
    "children":[
      "info-card#pruebader-b-tab"
    ],"props":{
      // "width": "45%"
    }
  },"info-card#pruebader-b-tab":{
    "props":{
      "id": "info-card-example",
        "isFullModeStyle": false,
        "textPosition": "left",
        "textMode":"rich-text",
        "headline": "# MAISON PAULETTE BISTRÓ",
        "subhead": "Espacios que cuentan historias de un mestizaje gastronómico a través de sabores y aromas del mundo.",
        "callToActionText": "VER MÁS",
        "callToActionUrl": "/storelocator",
        "textAlignment": "center",
        "blockClass":"inforpuebader"
    }
  },
  "flex-layout.row#boton-storelactor-boutiques":{
    "children":[
      "rich-text#link-storelocator-boutiques"
